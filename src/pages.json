{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
    "cloud(.*)": "@/components/cloud$1.vue"
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/home"
    },
    {
      "path": "pages/order/order"
    },
    {
      "path": "pages/my/my"
    },
    {
      "path": "pages/auth/login"
    },
    {
      "path": "pages/address/address"
    },
    {
      "path": "pages/address/selectCity"
    },
    {
      "path": "pages/search/search"
    }
  ],
  "globalStyle": {
    "navigationStyle": "custom"
  },
  "tabBar": {
    "color": "#777777",
    "selectedColor": "#ea7a99",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_select.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/order/order",
        "iconPath": "static/tabbar/order.png",
        "selectedIconPath": "static/tabbar/order_select.png",
        "text": "订单"
      },
      {
        "pagePath": "pages/my/my",
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my_select.png",
        "text": "我的"
      }
    ]
  }
}
